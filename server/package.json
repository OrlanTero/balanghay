  "version": "1.0.0",
  "description": "A modern library management system",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "dev": "electron-forge start",
    "package": "electron-forge package",
    "make": "cross-env NODE_ENV=production electron-forge make",
    "make:win": "cross-env NODE_ENV=production electron-forge make --platform=win32",
    "make:mac": "cross-env NODE_ENV=production electron-forge make --platform=darwin",
    "make:linux": "cross-env NODE_ENV=production electron-forge make --platform=linux",
    "publish": "cross-env NODE_ENV=production electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "build": "cross-env NODE_ENV=production webpack",
    "build:dev": "webpack",
    "watch": "webpack --watch",
    "rebuild": "npm run build && npm start",
    "reset-db": "node reset-database.js && npm start"
  },
  "keywords": [
    "library",
    "management",
    "system",
    "electron",
    "react"
